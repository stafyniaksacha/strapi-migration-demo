{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "version": {
      "type": "integer",
      "min": 1,
      "required": true,
      "default": 1
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "Draft",
        "In_Review",
        "Scheduled",
        "Published"
      ],
      "required": true,
      "default": "Draft"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    }
  }
}
